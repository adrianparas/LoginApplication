<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>register.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>com.snva.DashboardController</servlet-name>
    <servlet-class>com.snva.DashboardController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.snva.DashboardController</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>com.snva.RegistrationServlet</servlet-name>
    <servlet-class>com.snva.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.snva.RegistrationServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>com.snva.LogoutServlet</servlet-name>
    <servlet-class>com.snva.LogoutServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>com.snva.LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>



